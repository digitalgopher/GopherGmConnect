<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @*<link href='http://fonts.googleapis.com/css?family=Roboto:400,100,300,900' rel='stylesheet' type='text/css'>*@
    <link href='http://fonts.googleapis.com/css?family=Oswald:400,700,300' rel='stylesheet' type='text/css'>
    <link href="//netdna.bootstrapcdn.com/font-awesome/4.0.1/css/font-awesome.css" rel="stylesheet">
    <link href="//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    @*@Styles.Render("~/Content/fullcalendar")*@
    @Styles.Render("~/Content/style")
</head>
<body>
    @RenderBody()
    @Scripts.Render("~/bundles/modernizr")
    <script src="//ajax.googleapis.com/ajax/libs/jquery/2.0.3/jquery.min.js"></script>
    @*<script src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.3/angular.js"></script>
        <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.3/angular-route.min.js"></script>*@


    <script src="//cdnjs.cloudflare.com/ajax/libs/handlebars.js/1.3.0/handlebars.min.js"></script>
    @*<script src="http://builds.emberjs.com/release/ember.min.js"></script>*@
    <script src="http://builds.emberjs.com/beta/ember.js"></script>

    <script src="//cdnjs.cloudflare.com/ajax/libs/moment.js/2.5.0/moment.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/hogan.js/2.0.0/hogan.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/typeahead.js/0.9.3/typeahead.min.js"></script>
    <script src="//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js"></script>

    @*@Scripts.Render("~/bundles/angular")*@
    @*@Scripts.Render("~/bundles/fullcalendarjs")
        @Scripts.Render("~/bundles/charts")
        @Scripts.Render("~/bundles/typeahead")*@
    @Scripts.Render("~/bundles/ember")
    @Scripts.Render("~/bundles/ember/models")
    @Scripts.Render("~/bundles/ember/routes")
    @Scripts.Render("~/bundles/ember/helpers")
    @Scripts.Render("~/bundles/ember/controllers")
    @Scripts.Render("~/bundles/ember/views")
    @Scripts.Render("~/bundles/ember/components")
    @Scripts.Render("~/bundles/templates")
    @Scripts.Render("~/bundles/app")
</body>
@RenderSection("scripts", required: false)
</html>
