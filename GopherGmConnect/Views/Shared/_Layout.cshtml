<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,500,100,300,700,900' rel='stylesheet' type='text/css'>
    @*<link href="//netdna.bootstrapcdn.com/twitter-bootstrap/2.3.2/css/bootstrap-combined.min.css" rel="stylesheet">*@
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/emberanimation")
     @Styles.Render("~/Content/bootstrap3")
         @*@Styles.Render("~/Content/bootstrap")*@

    @Styles.Render("~/Content/style")

</head>
@RenderBody()

@Scripts.Render("~/bundles/jquery")
@*    @Scripts.Render("~/bundles/knockout")*@
@Scripts.Render("~/bundles/ember")

@*@Scripts.Render("~/bundles/bootstrap")*@
@Scripts.Render("~/bundles/bootstrap3")
@Scripts.Render("~/bundles/typeahead")
@Scripts.Render("~/bundles/app")
    

@Scripts.Render("~/bundles/ember/routes")
@Scripts.Render("~/bundles/ember/models")
@Scripts.Render("~/bundles/ember/controllers")
@Scripts.Render("~/bundles/ember/views")
@Scripts.Render("~/bundles/ember/helpers")

@RenderSection("scripts", required: false)
</html>
